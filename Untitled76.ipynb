      "source": [
        "n = int(input(\"Digite um número inteiro: \"))\n",
        "\n",
        "tem_adjacente = False\n",
        "\n",
        "x = -1\n",
        "\n",
        "while n > 0:\n",
        "    resto = n % 10\n",
        "    n = n // 10\n",
        "\n",
        "    if resto == x:\n",
        "        tem_adjacente = True\n",
        "        break\n",
        "\n",
        "    x = resto\n",
        "\n",
        "if tem_adjacente:\n",
        "    print(\"Sim\")\n",
        "else:\n",
        "    print(\"Não\")"
      ]
